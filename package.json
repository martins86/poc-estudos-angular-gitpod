{
  "name": "poc-estudos-angular-gitpod",
  "version": "1.4.0",
  "description": "Poc para estudos com Angular, rodando no gitpod",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "npm run build-local && lite-server",
    "build": "ng build",
    "build-local": "ng build --prod --aot --deploy-url ./ --base-href ./",
    "test": "ng test --no-watch --no-progress --code-coverage --browsers ChromeHeadless",
    "test-dev": "ng test --code-coverage --progress",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4201",
    "prettier-write": "npx prettier --write --ignore-unknown .",
    "prettier-check": "npx prettier --check .",
    "pre-commit": "npm run lint && npm run test && npm run e2e",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martins86/poc-estudos-angular-gitpod.git"
  },
  "keywords": [],
  "author": "Thiago Martins de Freitas",
  "email": "martins86@gmail.com",
  "url": "https://www.linkedin.com/in/tmfreitas/",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/martins86/poc-estudos-angular-gitpod/issues",
    "email": "martins86@gmail.com"
  },
  "engines": {
    "node": "^12.18.3"
  },
  "homepage": "https://martins86.github.io/poc-estudos-angular-gitpod/",
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.4",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "~11.2.4",
    "@angular/compiler": "~11.2.4",
    "@angular/core": "~11.2.4",
    "@angular/fire": "^6.1.0-rc.4",
    "@angular/forms": "~11.2.4",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "~11.2.4",
    "@angular/platform-browser-dynamic": "~11.2.4",
    "@angular/router": "~11.2.4",
    "firebase": "^8.2.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "^11.2.3",
    "@angular/compiler-cli": "~11.2.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^5.0.9",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "lite-server": "^2.6.1",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "@angular-devkit/architect": "0.1102.3",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
